{
    "name": "vnn/wordpress-rest-api-client",
    "description": "A wordpress rest api libary for laravel",
    "keywords": [
        "laravel",
        "wordpress"
    ],
    "homepage": "https://github.com/mauthi/wordpress-rest-api-client",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vnn\\WpApiClient\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "^8.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vnn\\WpApiClient\\AppServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    }
}
